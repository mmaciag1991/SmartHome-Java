<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>

<GridPane minHeight="800.0" minWidth="1200.0" styleClass="background" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sm.smarthome.Controllers.MainViewController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="128.0" minWidth="128.0" prefWidth="128.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints maxHeight="32.0" minHeight="10.0" prefHeight="32.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <VBox fx:id="LeftButtonsBox" prefHeight="200.0" prefWidth="100.0" styleClass="background" GridPane.rowIndex="1">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <GridPane.margin>
            <Insets top="10.0" />
         </GridPane.margin></VBox>
      <StackPane fx:id="PagePane" prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets left="10.0" top="10.0" />
         </GridPane.margin></StackPane>
      <GridPane fx:id="topGridPane" GridPane.columnSpan="2">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <HBox fx:id="topButtonsBox" alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" style="-fx-border-width: 0;" styleClass="menu-button" GridPane.columnIndex="1" />
            <Pane fx:id="userPane" prefHeight="200.0" prefWidth="200.0" style="-fx-border-width: 0;" styleClass="menu-button" />
         </children>
      </GridPane>
   </children>
</GridPane>
